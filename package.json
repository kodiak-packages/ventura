{
  "name": "ventura",
  "version": "0.0.0-development",
  "repository": "git@github.com:kodiak-packages/ventura.git",
  "author": "Kodiak",
  "license": "MIT",
  "private": false,
  "files": [
    "dist",
    "README.md"
  ],
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --detectOpenHandles --watchAll",
    "document": "docz build",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "release": "semantic-release",
    "copy:assets": "cpx './src/**/!(docAssets)/*.{css,svg}' 'dist' && cpx './src/*.{css,svg}' 'dist'",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16"
  },
  "devDependencies": {
    "@docusaurus/core": "^2.0.0-alpha.72",
    "@docusaurus/module-type-aliases": "^2.0.0-alpha.72",
    "@docusaurus/preset-classic": "^2.0.0-alpha.72",
    "@mdx-js/react": "^1.6.22",
    "@semantic-release/git": "^9.0.0",
    "@testing-library/dom": "^7.26.3",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.6",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-helmet": "^6.1.0",
    "@types/react-portal": "^4.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^4.17",
    "clsx": "^1.1.1",
    "cpx": "1.5.0",
    "docz": "^2.3.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-ban": "1.5.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-svg-transformer": "^1.0.0",
    "prettier": "^2.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-release": "^17.4.1",
    "ts-jest": "^26.4.4",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.1",
    "classnames": "^2.2.6",
    "react-compound-slider": "^3.3.1",
    "react-feather": "^2.0.9",
    "react-popper": "^2.2.4",
    "react-portal": "^4.2.1",
    "react-transition-group": "^4.4.1"
  },
  "keywords": [
    "kodiak",
    "ventura",
    "react",
    "ui",
    "component",
    "library"
  ],
  "release": {
    "branches": [
      "master"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy",
      "^.+\\.svg$": "jest-svg-transformer"
    }
  }
}
